<div class="" >
    {{#each beers}}
    <div class= "row d-flex justify-content-center">
        <div class="col-4">

        <img class="beer-img-top" src="{{this.image_url}}" alt="Beer card">
        <div class="beer-body">
            <h1 class="beer-title">{{this.name}}</h1>
            <h3 class="beer-title">{{this.tagline}}</h3>
            <p class="beer-text">{{this.description}}</p>
        </div>

        </div>
    </div>
    {{/each}}
</div>